<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.service.taskdoc">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".service.system.configure.GyenggyFont"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/img_main_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".display.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".display.activity.CreateUserActivity" />
        <activity
            android:name=".display.activity.ProjectActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".display.activity.HomepageActivity" />
        <activity
            android:name=".display.activity.MethodBoardViewActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".display.activity.MethodBoardAddActivity"
            android:label="게시판 등록"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".display.activity.MethodListActivity"
            android:label="내 방법론 리스트"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".display.activity.ProjectProgressActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".display.activity.ChatingActivity"
            android:label="채팅방"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".display.activity.GanttChartActivity"
            android:theme="@style/AppTheme"></activity>

    </application>

</manifest>